{
  "name": "couchloop-eq-mcp",
  "version": "1.0.2",
  "description": "Turn conversations into guided journeys that remember where you left off - MCP server for stateful AI experiences",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "couchloop-eq-mcp": "./dist/index.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "run-mcp-server.sh",
    "assets/"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext ts",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "setup": "npm install && npm run build",
    "setup:db": "npm run db:push && npm run db:seed",
    "vercel": "vercel",
    "vercel:prod": "vercel --prod",
    "server": "tsx src/server/index.ts",
    "server:dev": "tsx watch src/server/index.ts"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "chatgpt",
    "claude",
    "conversation",
    "state-management"
  ],
  "author": "Greg Wisenberg <greg@couchloop.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wisenbergg/couchloop-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/wisenbergg/couchloop-mcp/issues"
  },
  "homepage": "https://github.com/wisenbergg/couchloop-mcp#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@supabase/supabase-js": "^2.39.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.1",
    "express": "^5.2.1",
    "jose": "^5.2.0",
    "jsonwebtoken": "^9.0.3",
    "nanoid": "^5.0.4",
    "pg": "^8.16.3",
    "postgres": "^3.4.3",
    "uuid": "^13.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vercel/node": "^3.0.0",
    "drizzle-kit": "^0.20.7",
    "eslint": "^8.56.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vercel": "^32.0.0",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}
